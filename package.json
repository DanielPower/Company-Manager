{
  "name": "Employee Manager",
  "version": "0.0.0",
  "scripts": {
    "build": "node build.js",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/client"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.1.0",
    "http-status-codes": "^2.1.2",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "winston": "^3.3.3",
    "zapatos": "^0.1.54"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "^3.5.14",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.4",
    "@types/pg": "^7.14.4",
    "@types/supertest": "^2.0.10",
    "eslint": "^7.8.1",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.1",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
